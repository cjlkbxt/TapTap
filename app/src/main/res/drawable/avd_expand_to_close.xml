                <group
                    android:name="group_right"
                    android:translateX="-6"
                    android:translateY="6">
                    <path
                        android:name="right"
                        android:pathData="M 12 10.6 L 16.9 5.7 C 17.3 5.3 17.9 5.3 18.3 5.7 C 18.7 6.1 18.7 6.7 18.3 7.1 L 7.1 18.3 C 6.7 18.7 6.1 18.7 5.7 18.3 C 5.3 17.9 5.3 17.3 5.7 16.9 L 10.6 12"
