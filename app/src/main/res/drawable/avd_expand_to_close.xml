                    android:strokeColor="#979797"/>
                <group
                    android:name="group_left"
                    android:translateX="6"
                    android:translateY="6">
                    <path
                        android:name="left"
                        android:pathData="M 12 10.6 L 7.1 5.7 C 6.7 5.3 6.1 5.3 5.7 5.7 C 5.3 6.1 5.3 6.7 5.7 7.1 L 16.9 18.3 C 17.3 18.7 17.9 18.7 18.3 18.3 C 18.7 17.9 18.7 17.3 18.3 16.9 L 13.4 12"
